/**
 * ErrorCatalog - Definition of all error types for the werewolf game GM support library
 * 
 * Provides centralized error definitions with standard format including:
 * - Unique error codes (prefix + number)
 * - Human-readable messages
 * - Detailed descriptions
 * - Error levels
 */

/**
 * Error level definitions
 * @enum {string}
 */
const ErrorLevel = {
  FATAL: 'fatal',
  // Unrecoverable errors that prevent game continuation
  ERROR: 'error',
  // Regular errors requiring handler intervention
  WARNING: 'warning',
  // Issues that don't prevent operation but may indicate problems
  INFO: 'info' // Informational messages about potential issues
};

/**
 * Standard error structure
 * 
 * @typedef {Object} ErrorDefinition
 * @property {string} code - Unique error code (e.g., 'E0101')
 * @property {string} message - Human-readable error message
 * @property {string} details - Detailed explanation of the error
 * @property {string} level - Error severity level from ErrorLevel enum
 */

/**
 * Main error catalog organized by categories
 */
const ErrorCatalog = {
  // Player-related errors (E01xx)
  PLAYER: {
    INVALID_PLAYER_ID: {
      code: 'E0101',
      message: '存在しないプレイヤーID',
      details: '指定されたプレイヤーIDのプレイヤーが存在しません。',
      level: ErrorLevel.ERROR
    },
    PLAYER_ALREADY_EXISTS: {
      code: 'E0102',
      message: 'プレイヤー名が既に使用されています',
