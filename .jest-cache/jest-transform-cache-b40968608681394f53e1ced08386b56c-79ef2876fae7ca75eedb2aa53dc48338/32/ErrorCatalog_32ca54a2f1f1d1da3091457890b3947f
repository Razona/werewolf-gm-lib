/**
 * Get error definition by code
 * @param {string} code - Error code to look up
 * @return {ErrorDefinition|null} - Error definition or null if not found
 */
function getErrorByCode(code) {
  for (const category of Object.values(ErrorCatalog)) {
    for (const error of Object.values(category)) {
      if (error.code === code) {
        return error;
      }
    }
  }
  return null;
}

/**
 * Get all error definitions for a specific level
 * @param {string} level - Error level from ErrorLevel enum
 * @return {Object[]} - Array of error definitions matching the level
 */
function getErrorsByLevel(level) {
  const result = [];
  for (const category of Object.values(ErrorCatalog)) {
    for (const error of Object.values(category)) {
      if (error.level === level) {
        result.push(error);
      }
    }
  }
  return result;
}
module.exports = {
  ErrorCatalog,
  ErrorLevel,
  getErrorByCode,
  getErrorsByLevel
};
